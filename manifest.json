{
    "name" : "Autotagger",
    "version" : "0.1.1",
    "description" : "Automatically add tags based on a predefined set of rules",
    "content_scripts": [{
	"matches" : ["http://www.tumblr.com/new/*",
		     "http://www.tumblr.com/edit/*",
		     "http://www.tumblr.com/reblog/*"],
	"js" : ["jquery-1.6.1.js", "autotagger.js"]
    }],
    "icons" : {
	"128" : "icon128.png",
	"48" : "icon48.png",
	"16" : "icon16.png"},
    "options_page" : "options.html",
    "background_page" : "background.html"
}